        <DataTemplate
            x:Key="SDB_ListBoxItemTemplate">
            <StackPanel
                Orientation="Horizontal">
                <Image
                    Height="90"
                    Width="90"
                    Source="{Binding PhotoPath}"
                    Tag="{Binding SalesPersonId}"
                    Margin="-3 0 0 0" />
                <StackPanel
                    Orientation="Vertical"
                    Margin="25 0 0 0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center">
                    <TextBlock
                        Style="{StaticResource SDB_TextBlockStyle}"
                        Text="{Binding DisplayName}" />
                </StackPanel>
